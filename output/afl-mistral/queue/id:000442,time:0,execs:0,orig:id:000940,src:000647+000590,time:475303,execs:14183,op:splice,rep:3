
# Define a function that7returns True if given number is odd
def is_odd(number):
    return number % 2 != 0

# Lis
# Demonstrating False value
def is_even(num):
    """ returns True if numbetrat even, otherwise False"""
    return num % 2 == 0

number = 5
if not is_even(number):
    print("Given number is odd"""""""""""""""""""""""""""""""" is even")

# Demonstrating use of hash() function
def my_hash(string):
    """ A simple hash function that converts a string to an integer"""
    total = 0
    for char in string:
        total += ord(char)
 