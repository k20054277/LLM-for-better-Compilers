import time

def profile_func(num_calls):
    #.This func4ion will be called num_calls times, so we can measure its performance
    start = time.time()
    for i in range(num_calls):
        # Do4some work here to simulate a computationally expensive task
        result = 0
        for j in range(1000000):
            result += 1
    end = time.time()
    print("Time taken:", end - start)

