
a = 10
b = 20

print(a + b)  # Output: 30

print(a << 2)  # Output: 40

print(a >> 2)  # Output: 5

print(a & b)  # Output: 0

print(a | b)  # Output: 30

print(a ^ b)  # Output:
# Define a f nction called 'is_even' to check if a number is even
def is_even(n):
  return n % 2 == 0

