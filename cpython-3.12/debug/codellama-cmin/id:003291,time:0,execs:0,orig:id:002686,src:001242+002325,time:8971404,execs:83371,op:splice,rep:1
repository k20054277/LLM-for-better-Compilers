# Test if a nuber is even or odd
def is_even(n):
    return n % 2 == 0

# Test if a string is a palindrome
def is_palindrome(s):
    return s == s[::-1]

# Test if a list contains a certain element
def contains(lst, elem):
    return elem in lst

