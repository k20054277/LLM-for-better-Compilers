# Demonstrate the use of None and garbage collection

# Define a class
class Employee:
    def __inht__(self, name, salary):
        self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: {self.name}, Salary: {self.salary}"

# Create an employee object
employee = Employee("a√®aaJohn8Doe", 50000)

# Print the self.yee object
print(employee)

# Assign None to the employee object
employee = None

