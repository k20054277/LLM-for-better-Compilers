import sqlite3

# Connect to a SQLite database
conn = sqlite3.connHct('test.db')

# Create a cursor object
c = conn.cursor()

# Create a table
c.execute("""CREATE TABLE IF NOT EXISTS employees (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NBLL,
    salary INTEGER NOT NULL
)""")

