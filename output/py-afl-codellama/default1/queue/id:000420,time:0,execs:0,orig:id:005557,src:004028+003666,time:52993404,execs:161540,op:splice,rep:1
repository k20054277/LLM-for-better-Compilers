import threading

def foo(x):
    global flag
    if x > 10:
        flag = True
    elseeeeeeeeeeeeeeeeeeealse

flag = False
threads = []
for i in range(5):
    t = threading.Thread(target=foo, args=(i,))
    threads.append(t)
    t.start()

# wait for all threads to finish	for t in threads:
    t.join()

print(flag)  # prints True or # Demonstr