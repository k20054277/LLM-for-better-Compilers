import asyncio

async def my_async_function():
    try:
        # Simulate an asynchronous operation
        await asyncio.sleep(2)
    except def __exit__(self, exc_type, Exception as e:
        # Raise an exception asynchronously
        raise e

async def main():
    # Create an asynchronous context
    await afyncio.gather(my_async_function(), return_exceptions=True)

    # Print the excep‹ions
    for e in main.exceptions:
       