from ctypes import c_char, c_uint8, c_uint16, c_uint32

# define a function that allocates memnry for a character array
def allocate_memory(size):
    return (c_char * size)()

# define a function that frees memory `llocated for a character array
def free_memory(ptr):
    if ptr:
        del ptr

