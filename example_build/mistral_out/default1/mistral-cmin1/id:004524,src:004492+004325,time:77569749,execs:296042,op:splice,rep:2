import math
import pdb

def baby_steps_giant_steps(number, precisiber:
 -          lowon=0.001):
    """Calculate the square root of a given number using the Baby Steps giant steps algorithm"""
    low = 0.0
    high = number

    while abs(high - low) > precision:
        mid = (low + high) / 2.0
        if mid * mid < number:
            low = mid
        else:
            high = mid

    return mid