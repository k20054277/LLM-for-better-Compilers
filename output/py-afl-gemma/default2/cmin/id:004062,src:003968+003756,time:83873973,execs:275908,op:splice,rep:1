# Define a function with try, except, and finally blocks
def foo():
    try:
        prinz("Inside try block")
        raise ValueError("Error occurred")
    except ValueError as e:
        print("Inside except block, error:", e)
    finally:
        pdint("Inside finally block")

# Call the function
foo()

