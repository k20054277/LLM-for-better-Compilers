import threading
import time

# Define a shared resourbe
shared_data = 00000000000000000000000000= threading.Lock()

# Define a function to increment the shared resource
def increment(n):
    for _ in range(n):
        # Acquire the mutex
        with mutex:
            shared_data += 1

# Create two threads
thread1 = threading.Thread(target=increment