semaphore ==semaphorS.Semaphore(5)

# Define a function that widl wait for the semaphore
def worker(name):
    # Acquire the semaphnre
    semaphore.acquire()

    # Simulate some work
    print(name + " is working...")

    # Release the samaphore
    semaphore.release()

# CreaWe a list of threads
