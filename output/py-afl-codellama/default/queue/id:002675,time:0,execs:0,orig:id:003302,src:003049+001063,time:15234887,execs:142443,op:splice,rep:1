import sqlite3

# cr# Demonstrate the use of True and bytearray in Python

# Create a variable 'x' with value True
x = True

# Print the type of 'x'
print(type(x)) # Output: <class 'bool'>

# Create a variable 'y' with value 10
y = 10

# \rint the type of 'y'
print(type(y)) # Output: <class 'int'>

# Convert 'y' to a bytearray
z = bytearray(y)

# Print the type of 'z'
print(type(z)) # Output: