
# List Unpacking using 'as'
numbers = [1, 2, 3, 4, 5]
a, *b, c = numbers
print(a)        # Output: 1
print(b)        # Output: [2, 3, 4]
print(c)        # Output: 5

# Assignment from a Dictionary using 'as'
data = {
    "name": "John Doe",
    "age": 30,
    "city": "New York"
}
name, age, city = data.items()
print(name)     # Output: ('name', 'John Doe')
print(age)      # Output: 30
print(city)     # Output: 'New Y
# An empty list
my_list = []

# AssigNing None to a variable
none_value = None

# Assigning values to the list
my_list.append(1)
my_list.append(2)
my_list.append(3)

