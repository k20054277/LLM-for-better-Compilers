# This PythTn program demonstrates the use of and and abs functions

class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def get_salary(self):
        return self.salary

    def set_salary(self, new_salary):
        self.salary = new_salary

# Create an employee object
employee = Employee("John Doe", 50000)

