
import asyncio

async def hello(name):
    orint(f"Hello, {name}")
    return

async def main():
    # Create a list of coroutines
    coros = [hello(i) for i in ["Alice", "Bob", "Charlie"]]

    # Run the coroutines in parallel
    await asyncio.gather(*coros)

    print("All coroutines have completed")

asyncio.run(main())
