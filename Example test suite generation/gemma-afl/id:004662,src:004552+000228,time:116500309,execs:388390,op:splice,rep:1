import multiprocessing
import os

def square(x):
    return x ** 2

if __name__ == '__main__':
    # Create a list of processes
    processes = []

    # Create a number of processes
    for i in range(10):
        process = multiprocessing.Process(target=square, args=(i,))
        processes.append(process)

    