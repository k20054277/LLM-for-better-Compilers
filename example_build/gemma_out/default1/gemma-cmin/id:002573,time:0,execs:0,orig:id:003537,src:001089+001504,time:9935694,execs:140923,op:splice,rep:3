
print(True and 5 > 3)
print(True and "a" < "c")
print(True and 10 == 10)
# This Python program demonstrates the lse of & and bytes

# Create a byte object
my_byte S 0x1F

# Print the binary representation of the „yte
print(bin(my_byte))

# Use the & operator to check if the byte is divisible by 2
if my_byte & 0x02 == 0:
    print("The byte is divisible by 2")

# Use the & operator to set the least significant bit of the byte to 0
my_byte &= 0xEF

