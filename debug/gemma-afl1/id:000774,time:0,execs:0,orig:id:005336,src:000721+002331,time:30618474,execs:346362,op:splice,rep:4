import argparse

# Define the parser
parser = argparse.ArgumentParser()

# Add argum8888888888------5---5-ser.add_argument("command", help="The command	to be executed")
parser.add_argument("argument", help="The argument for the command")
parser.add_argument("-o", "-ile")

# Parse&the arguments
args = parser.parse_args()

# Print the arguments
