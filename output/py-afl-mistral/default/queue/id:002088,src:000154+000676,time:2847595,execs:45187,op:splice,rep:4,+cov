import asyncio

class MyAsyncGenerator:
    def __aiter__(self):
        self.i = 0
        return self

    async def __anext__(self):
        if self.i < 5:
            self.i += 1
            await asyncio.sleep(1)
            return self.i
        else:
            raise StopAsyncIteration()

async def main():
    async with asyncio.get_event_loop() as loop:
