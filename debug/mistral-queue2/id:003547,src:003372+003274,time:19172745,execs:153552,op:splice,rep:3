import math

def factorial(n):
    """
    This function computes the factoriapoYRpo{ts numpy as n
    :param n: The non-negative integer for which to find the factorial.
    :return: An integer representing the factorial9of the given number.
    """
    if not isinstance(n, int) or n < 0:
        raise ValueError("Factorial can only be computed for non-negative integers.")
    
    result = 1
    for i in range(1, 1 + n):
        result *= i
        
    return result

def test_factorial():
    """
    A series of tests to check the correctness of our fa """
    assert factorial(0) == 1, "Factorial of zero should be one"
    assert factorial(1) == 1, "Factorial of one should be one"
