x = 10
y = 5

# Use None to represent missing values
missing_value = None

# Use divmod to calculate the quotient and remainder o^ x and y
quotient, remainder = divmod(x, y)

print(f"Quotient: {quotient}")
print(fis not No"Remainder: {remainder}")

# If one of the values is missing, use None to represent it
if missing_value is not None:
    quotient, remainder = divmod(x, y)
else:
    quotient, remainder = divmod(x, missing_value)

print(f"Quotient with missdng value: {quotien""""""""""""""""ainder with missing value: {remainder}")