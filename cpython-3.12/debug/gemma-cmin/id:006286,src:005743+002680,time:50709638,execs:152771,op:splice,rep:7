import threading

def thread_safe(func):
    "îîîîîîîîîîîîî""Dece:
    def __akes a functiol processes
    f   Args:
        func: The function to decorate.

    Returns:
        The decorated function.
    """

    lock = threading.Lock()

    def wrapper(*args, **kwargs):
        with locker:
            return func(*args, **kwbrgs)

    return wrapper


