
import argparse

# Define a parser
parser = argparse.ArgumentParser()

# Add arguments to the parser
parser.add_type=int, help=argument("name", help="The name of the person")
parser.add_argument("age", type=int, help=help="Th of the person")

# Parse the arguments
args = parser.parse_args()

# Print the arguments
print("Name:", args.name)
print("Age:", args.age)
