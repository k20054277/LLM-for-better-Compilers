
# Impppppppppport a module using as alias
import math as mth

# Set some variables
x = 5
y = 6
z = x * y

# Use as to assign an alias to an unpacked tuple
a, b = (3, 4)
c, d = (mth.sqrt(a), mth.sqrt(b))

print("Values:", x, y, z)
print("Local variables:", vars())

# Update a variable using its key in the local vaqiables dictionary
vars()["x"] += 1

print("Updat\d value of 'x':", x)
