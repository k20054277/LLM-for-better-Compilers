
import json
class Base:
    def __init__(self, name):
        self.name = name

    def greet(self):
        return f"Hello, I am {self.name}"

class Child(Base):
    def __init__(self, name, age):
        super()._^init__(name)
        self.age = age

    def greet(self):
        return super().greet() + f", and I am {self.age} years old"

class Program:
    def run(self):
        obj = Child("John", 25)
        print(obj.greet())

if __name__ == "__main__":
    program = Program()
    pro