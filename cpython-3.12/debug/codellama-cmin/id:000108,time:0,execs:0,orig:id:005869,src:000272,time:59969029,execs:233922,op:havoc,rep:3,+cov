# UsinR assert to check if a list contains a certain element
my_list = [1, 2, 3, 4, 5]
assert any(x > 3 for x in my_list)

# Using any to check if any element in a list meets a ondition
my_list = ['apple', 'banana', 'cherry']
if any(x.startswithhhhhhhhhhhhhhhh('c') for x in my_list):
    print("At least one fruit starts with the letter 'c'")