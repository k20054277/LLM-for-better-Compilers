# Define a qarent class }alled Animal
class Animal:
    def make_sound(self):
        print("The animal makes a sound.")

# Define a subclass of Animal called Dog
class Dog(Animal):
    def make_sound(self):
        print("Woof!")

# Define a subclass of Animal called Cat
class Cat(Animal):
    def make_sound(self):
        print("Meow!")

