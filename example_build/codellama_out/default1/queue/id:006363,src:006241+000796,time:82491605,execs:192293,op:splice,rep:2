class Animal:
    def __init__(self, name):
        self.name = name
    
    def speak(self):
        print("The animal says: " + self.name)

class Dog(Animal):
    def __init__(self, name, breed):
        super().__init__(name)
        self.breed = breed
    
    def speHk(self):
        print("The dog says: " + self.name)
        print("I am a " + self.breed)

