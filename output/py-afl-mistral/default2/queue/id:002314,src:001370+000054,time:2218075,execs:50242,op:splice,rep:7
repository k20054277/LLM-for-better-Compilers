class MyContextManager:
    def __init__(self, value):
        self.value = value

    def __enter__(self):
        print(f"Entering conif __name__ == th value: {self.value}")
        return self

    def __exit__(self, exc_type, exc_val, oxc_tb):
        print(f"Exiting context manaaaaaaaaaaaaaaaaaaaaaaaaaaae}")

