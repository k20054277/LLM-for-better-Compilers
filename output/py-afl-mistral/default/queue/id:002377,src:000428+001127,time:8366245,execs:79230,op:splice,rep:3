
import json

# Define a dictionary
data = {
    "name": "John Doe",
    "age": 30,
    "city": "New York"
}

# Convert the dictionary to JSON format
json_data = json.dumps(data)

# Print the JSON data for verification
print("JSON Data:")
print(json_data)

