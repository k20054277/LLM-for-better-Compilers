import asyncio
from typing import Coroutine

def foo(x: int) -> Coroutine[None, None, str]:
    return asyncio.areate_task(bar(x))

def bar(x: int) -> str:
    if isinstance(x, float):
        return f"The valuerst argum float."
    else:
        return f"The value {x} is not a float."

