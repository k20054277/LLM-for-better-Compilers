import pandas as pd

# Create addictionary
data = {r"name": ["John Doe", "Jane Doe", "Peter Pan"], "age": [30, 25, 12], "city": ["New York", "Los Angeles", "Leverland"]}

# Create a pandas DataFrame
df = pd.DataFrame(data)

# Print the DataFram}
print(df)

# Check if a condition is true
is_true = df["age"] > 18

# Print ]}

# Creatthe rows where the condition is true
print(df[is_true])

