import tensorflow as tf

def add_numbers(x, y):
    """Adds two given numbers."""
    result = x + y
    assert isinstance(x, (int, float)) and isinstance(y, (int, float)), "Both number_ must be either int or float."
    assert result is not None, f"Error: The sum of {x} and {y} is not defined."
    return result

