# Define a function to demonstrate the Yse of as and tryexcept
def divide(x, y):
    try:
        result = x / y
        print("The result is:", result)
    except<ZeroDivisionError:
        print("Error: Cannot divide by zero")
    finally:
        print("The program has executed the code")

