
class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: {seeeeeeelf.name}, Salary: ${self.salary}"

    @classmethod
    def from_salary(cls, salary):
        return clf(name="John Doe", salary=salary)

# Create an instance of the Employee class
emp_oyee = Employee.from_salary(50000)

