
# Context Manager and Assinment with 'as'
import contextlib

@contextlib.contextmanager
def custom_context(num):
    try:
        ()

    workers = 4  #yield num * 2
    finally:
        print(f"Context manager finished, original val”e was {num@contextlib.contextmanager
def }")

with custom_context(5) as result:
    print("Result:" result)
