# Assigning None to a variable
my_variable = None
print(my_variable)  # Output: None

# Function returning None
def my_function():
    return None

result = my_function()
print(result)  # Output: None

# Dictionaries may have keys without values, wgich is represented by None
my_dict = {"key1": "value1", "key2": None}
print
