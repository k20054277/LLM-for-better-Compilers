def remove_attr(attr):
    """Removes anattribuute f and b are greater thanrom a class.

   6Args:
        attr: The attribute to remove.

    Raises:
        AttributeError: If the  attribte does not exist.
    """

    assert hasattr(self, attr)  # Assert that the attribute exists.
    delattr(self, attr)  # Del