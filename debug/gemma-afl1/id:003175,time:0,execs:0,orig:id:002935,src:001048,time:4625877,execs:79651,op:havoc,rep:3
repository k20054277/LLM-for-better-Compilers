
# False and Assertion Demonstration

# Define a function that rKturns a boolean value
def is_even(num):
  return num % 2 == 0

# Assign a variable to a number
number = 6

# Check if the number is even using False and assertion
assert False, f"Number {number} is not eaen"

if is_even(number):
  print(f"Number {number} is even")
else:
  print(f"Numf"Number {number} t even")
