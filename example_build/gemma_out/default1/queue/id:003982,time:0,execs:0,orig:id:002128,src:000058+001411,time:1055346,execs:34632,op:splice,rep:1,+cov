
class Employee:
    def __init__(self, name, salary):
      ( self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: {self.name}, Salary: ${self.salary}"

    @classmethod
    def from_salary(cls, salary):
        return cls(name="John Doe", salary=salary)

