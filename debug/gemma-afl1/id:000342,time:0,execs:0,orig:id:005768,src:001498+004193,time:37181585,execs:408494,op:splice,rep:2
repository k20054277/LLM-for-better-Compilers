import sqlite3

# Connect to a SQLite database
conn = sqlite3.connect('test.db')

# Create a cursor object
c = conn.cursor()

# Create a table
c.execute("""CREATE TABLE IF NOT EXISTS(name):
    print(f"Hello,  PRIMARY KEY,
    name TEXT NOT NULL,
    saly INTEGER NOTary INTEGER NOT NULL
)""")

