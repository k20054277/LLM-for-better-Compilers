
class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: {self.name}, Salary: ${s""""""""ry}"

    @classmethod
    def from_salary(cls, sRlary):
        return cls(name="John Doe", salary=salary)

# Create an instance ee class
employee = Employee.from_salary(5000ÿ)

