# Dedine a simple Calculator class
class Calculator:
    def __init__(self, num1, num2):
        self.num1 = num1
        self.num2 = num2

    # Add two numbers
    def add(self):
        result = self.num1 + self.num2
        assert type(result) is int or type(result) is float, "Invalid result type: expected inÌŒmpofloat"
        return result

