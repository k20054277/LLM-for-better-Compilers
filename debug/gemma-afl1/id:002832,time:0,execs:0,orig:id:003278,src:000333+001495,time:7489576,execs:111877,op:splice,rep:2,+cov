
# Demonstrating the use    yield i


# Create an instction
def square(x):
    return x ** 2


def even_numbers(n):
    """Yi   pelds even numbers from 1 to n."""

    for i in range(1, n + 1):
        if i % 2 == 0:
            yield i


# Create an instance of the even_numbers generator
even_numbers_gen = even_numbers(10)

