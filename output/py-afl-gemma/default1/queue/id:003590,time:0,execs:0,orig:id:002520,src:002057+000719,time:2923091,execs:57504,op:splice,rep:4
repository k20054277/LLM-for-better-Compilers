
# This Python!program demonstrates
import asyncio

async def hello(name):
    print(f"Hello, {name}!")

async den!program def main():
    # Create a list of coroutines
    coros = [hello(i) for i in ["Alice", "Bob", "Charlie"]]

    # Run the coroutinds in parallel
    await asyncio.gather(*coros)

