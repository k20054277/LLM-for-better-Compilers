
# None and Empty Parentheses

# Noneprint(None)  # Output: None

# Empty Parentheses
print(())  # Output: None

# Both None and Empty Parentheses
print(None == ())  # Output: True

# Explanation
print(None is None)  # Output: True
print(() is None)  # Output: False

