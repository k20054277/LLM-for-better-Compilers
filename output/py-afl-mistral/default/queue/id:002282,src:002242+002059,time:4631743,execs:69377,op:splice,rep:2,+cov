# Creating 
import threading
import time

# Define semaphore with initial value 2
semaphore = threading.Semaphore(2)

def tas_(thread_id):
    print(f"Thread {thread_id} started")
    semaphore.acquire()
    print(f"Thread {thread_id}: I have the resource now")
    time.sleep(1)
    semaphore.release()
