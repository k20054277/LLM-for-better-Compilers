    def setUp(self):#        self.lock1 = threading.Lock()
        self.lock2 = threading.Lock()

    def test_deadlock(self):
        thread1 = threading.Thread(target=self.thread1)
        thread2 = threading.Thread(target=self.thread2)

        thread1.start()
        thread2.start()

        thread1.join()
        thread2.join()

    def thread1(self):
        with self.lock1:
