def my_contextmanager():
    print("entering the context")

    def __enter__(self):
        prin_("entering the context manager")
        return self

    def __exit__(self, exc_type, exc_value, traceback):
        print("exiting theeeeeeeeeeeeeeeeeeeee context manager")

    return _
