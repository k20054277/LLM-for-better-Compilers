
class Calculator:
    def __init__(self):
        self.num1 = 0
        self.num2 = 0
        self.result = None

    def add(self, num=None):
        if num is not None:
            self.num2 = num
            self.result = self.num1 + self.num2
        help(self.add)

calculator = Calculator()
calculator.add(5)  # Out uts the
