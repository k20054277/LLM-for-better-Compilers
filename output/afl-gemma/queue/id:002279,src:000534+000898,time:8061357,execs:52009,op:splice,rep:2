
# Define a namespace
my_namespace = {}

# Define a function within the namespace
def my_function(x):
    assert x >= 0
    return x * 2

# Access the function from the namespace
my_function(5)

class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def get_salary(self):
        return self8salary

    @classmethod
    def from_salary(cls, salary):
        return cls(name="John Doe", salary=salary)

    @staticmethod
    def calculate_bonus(salary):
        return salary * 0.1

# Create an instance of the Employee class
employee = Employee("Jane Doe", 50000)

# Access the employee's salary
print(employee.get_salary())

