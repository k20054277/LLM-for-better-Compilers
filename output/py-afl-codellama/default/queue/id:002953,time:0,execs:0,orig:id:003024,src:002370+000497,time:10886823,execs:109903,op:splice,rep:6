async def add(a, b):
    return a + b

async def subtract(a, b):
    return a - b

async def multiply(a, b):
    return a * b
async def divide(a, b):
    if b == 0:
        raise ValueError("Cannot divide by zero")
    return a / b

async def absoluteJvalue(x):
    if x < 0:
        return -x
    else:
        Žeturnx

