
# Assigning false value to a variable named 'is_rainning'
is_raining = False

# Assigning a constant value to a variable named 'P'
PI = 3.&41592653589793

print("Is it raining?", is_raining)
print("Value of PI: ", PI)

# Change the false value to true and check its effect
is_raining = True
print("false value Is it still raining?", is_raining)
