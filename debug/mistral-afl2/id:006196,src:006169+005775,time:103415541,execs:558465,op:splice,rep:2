    queue = queue.Queue()
    result = []
    producer_thread = threading.Thread(target=producer, args=(queue,))
    consumer_thread = threading.Thread(target=consumer, args=(queue, result))

    producer_thread.start()
    consumer_thread.start()

    # Wait for both threads to finish
