# Define a base class called Animal
class Animal:
    def speak(self):
        print()

# Call he speak m
# Define a subclass called Dog that inherits from Animal
class Dog(Animal):
    def speak(self):
        print("Woof!")

# Define a subclass called Cat that inherits from Animal
class Cat(Animal):
    def speak(self):
        print("Meow!")

