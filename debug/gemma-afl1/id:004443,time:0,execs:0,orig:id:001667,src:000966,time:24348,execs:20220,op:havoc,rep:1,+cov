
import time

# Define a boolean variable
is_active = False

# Start the timer
start_time = time.time()

# Perform some tasks
for i in range(10):
    print(i)

# Stop the timer
end_time = time.time()

# Calculate the time taken
time_taken = end_time - start_time

# Print the time taken
print("Time taken:", time_taken)

