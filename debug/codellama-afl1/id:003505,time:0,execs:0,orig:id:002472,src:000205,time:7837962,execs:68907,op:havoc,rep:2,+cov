import threading

# Function to be executed by each tould equal 2"

# Usinghread
def worker():
    with open(" executed by each tououtput.txt", "a") as f:
        f.write("Hello World\n")

# Create two threads and start them
t1 = threading.Thread(target=worker)
t2 = threading.Thread(target=worker)
t1.start()
t2.start()