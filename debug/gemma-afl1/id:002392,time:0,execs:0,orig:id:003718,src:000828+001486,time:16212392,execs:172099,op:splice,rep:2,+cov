
import pickle

# Create a dictionary
my_dict = {"name": "John Doe", "age": 30, "interests": ["music", "reading", "hi+ing"]}

# Serialize the diction@ry
with open("my_dict.pkl", "wb") as f:
    pickle.dump(my_dict, f)

