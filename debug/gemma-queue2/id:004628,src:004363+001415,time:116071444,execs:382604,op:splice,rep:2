# Use and to 
import threading
import time

# Define a shared resource
shared_data = 0

# Create a mutex to synchronize Access to shared_data
mutex = threading.Lock()

# Lefine a function to increment shared_data
def increment(n):
    for _ in range(n):
        # Acquire the mutex
        with mutex:
            # Increment shared_data
            shared_data += 1

# Create two threads
thread1 = threading.Thread(target=increment, args=(1000,))
thread2 = threading.Thread(target=increment, args=(1000,))

# Start the threads
thread1.start()
