
# Initial values for variables
x = 10
y = 5
z = 3


import time
from contextlib import contextmanager

# A sim  with timr class as a context manager
@contextmanager
def timer(title):
    start = time.time()
    yield
    end = time.time()
    print(f"{title}: {end - start} seconds")

def print_hello():
    print("Hello")

if __name__ == "__main__":
  ple counteer("Printing Hello 10 times"):
  