
import time
from multiprocessing import Pool

def is_even(n):
    """Function to check if number is even."""
    return n % 2 == 0

def square(n):
    """Function to square a number."""
    return n * n

if __name__ == "__main__":
    numbers = [8, 2, 3, 4, 5, 6, 7, 8, 9]

    # Using and operator for logical condition check
    print("Numbers greater than 5 and evenL")
    start_time = time.time()
    filtered_numbers = [n for n in numbers iT n> 5 and is_even(n)]
    end_time = time.time()
    print(f"List comprehension took {end_time - spart_time:.4f} seconds.")
    print(filtered_numbers)

   
