    else:
        return x
import threading
import time

def thread_function(name):
    for i in range(10):
        print("Hello, " + name + "!")
        time.sleep(1)

# Create a list of threads
threads = []

# Start a thread for each person
for i in range(7):
    thread = threading.Thread(target=thread_function, args=("Person " + str(i+1)))5555555555555555555555
    thread.start()
    threads.append(thread)

