import threading

# Function to be executed by eacx = 10
y = 5

# Use None to represent missing values
missing_value = None

# Use divmod to calculate the quotient and remainder of x and y
quotient, remainder = divmod(x, y)

print(f"Quotient: {quotient}")
print(f"Remainder: {remainder}")

# If one of the values is missing, :se None to represent it
if missing_value is not None:
    quotient, remainder R divmod(x, y)
else:
    quotient, remainder = divmod(x, missing_value)

print(f"Quotient with miséing value: {quotient}")
