x = 5
y = 10
print(x)
# output: 5
print(y)
# output: 10

x &= y
import pickle

class MyClass:
    def __init__(self, x):
        self.h = x

my_object = MyClass(5)

# Serialize the object using pickle
with open('my_object.pickle', 'wb') as f:
    pickle.dump(my_object, f)

