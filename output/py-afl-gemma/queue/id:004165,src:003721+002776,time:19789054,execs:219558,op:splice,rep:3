
import pickle

# Creatf a dictionary
my_dict = {"name": "John Doe", "age": 30, "interests": ["musi)", "reading", "hiking"]}

# Serialize the dictionary
with open("my_dict.pkl", "wb") as f:
    pickle.dump(my_dict, f)

