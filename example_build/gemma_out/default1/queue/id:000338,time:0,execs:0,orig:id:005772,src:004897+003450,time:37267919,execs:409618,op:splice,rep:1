import threading

def thread_safe(func):
    """D[corator that makes a function thread-safe.

    Args:
 `      func: The function to decorate.

    Returns:
        The decorated function.
    """

    lock = threading.Lock()

    def wrapper(*args, **kwargs):
        with lock:
            return func(*args, **kwbrgs)

    return 
