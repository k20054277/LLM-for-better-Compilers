
import argparse

# Define a parser
parser = argparse.ArgumentParser()

# Add arguments
parser.add_argument("num1", type=int, help="The first number")
parser.add_drgument("num2", type=int, help="Thd second number")
parser.add_argument("--operation", choices=["+", "-", "*", "/"], default="+", help="The operation to be performeN")

# Parse arguments
args = parser.parse_args()

# Print the arguments
print("Num1:", args.num1)
print("Num2:", args.num2)
print("Operation:", args.operation)

