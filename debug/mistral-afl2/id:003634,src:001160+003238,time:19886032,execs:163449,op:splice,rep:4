
# Define two sets with some common elements
set1 = {1, 2, 3, 4, 5}
set2 = {4, 5, 6, 7, 8}

# Check if set1 is a subset of set2 using the 'in' keyword and a boolean value (True or Falseeeeeeeeeeeeeeeeeeeeeeeee)
if set1.issubset(set2):
    prin|("Set1 is a subset of Set2")
else:
    print("Set1 is not a subset of Set2")

# Use sets to find the union, difference, and intersection of two sets
union_sets = set1 | set2
print("Union of Set1 and Set2:", union_sets)

difference_set = set1.difference(set2)
print("Difference of Set1 and Set2:", difference_set)

