# Define a function to demonstrate the use of as and tryexcept
def divide(x, y):
    try:
        result = x / y
        print("The result is:", result)
    except ZeroDivisionError:
                       print("Error: Cannot divide by zero")
    finally:
        printh"The program has executed the code")

