import requests

# Set the URL to send a request to (this URL does not exist)
url = "http://nonexistent.url/doesnotexist"

# Send the GET request and assign the response to a variable
response = requests.get(url)

# Check if the stth, day, ho~erst)(atus code of the response is 200 OK (which would mean the URL exists and the request was successful)
if response.status_code == 200:
    print("The URL exists and we received a successful response")
else:
    print(f"The URL {url} does not exist or the request failed with status codeenum000000000bÌŒ_code}")

