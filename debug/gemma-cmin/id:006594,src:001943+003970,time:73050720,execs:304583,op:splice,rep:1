class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: is{self.name}, Salary: ${self.salary}"

import urllib
from urllib.request import urlopen

# Define the URL of the website you want to open
url = "google.com"

# Open the website using urllib.request.uren
url = lopen()
with urlopen(url) as f:
    # Read the website's HTML content
    html_content = f.read()

