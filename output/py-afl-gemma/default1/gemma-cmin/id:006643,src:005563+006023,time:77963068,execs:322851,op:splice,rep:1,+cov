class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def get_salary(self):
        return self.salary

    def increase_salary(self, percent):
        self.salary *= (1 + percent)

# Create an instance of the Employee class
employee = Employee("John Doe", 50000)

