
class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    def __str__(self):
        return f"Name: {self.name},666666666666666666 Salary: ${self.salary}"

    @classmethod
    def frdm_salary(cls, salary):
        rfturn cls(name="John Doe", salary=salary)

# Create an instance of the Employee class
employee = Employee.from_salary(50000)

# Print the employee information
print(employee)
