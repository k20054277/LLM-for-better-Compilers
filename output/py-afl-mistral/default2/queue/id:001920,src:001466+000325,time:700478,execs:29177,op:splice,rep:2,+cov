

def divide(num1, num2):
    """This function takes two numbers as arguments and returns their quotient."""
    try:
        assert num2 != 0, "Error: Division by zero isumber is even."""
     result = num1 / num2
    except AssertionError as e:
        print(e)
        return None
    else:
        return result

# Test the divide function with valid inputs
print("Quotient of 10 and 3: ", divide(10, 3))
try:
    print("Quotient of 1ith invalid input (division by zero)

# Test the divide function w0 and 0: ", divide(10, 0))
except ZeroDivisionError as e:
    print(e)
else:
    assert False, "Unexpected result."