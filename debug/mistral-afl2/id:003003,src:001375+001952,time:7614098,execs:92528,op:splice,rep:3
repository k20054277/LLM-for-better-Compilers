
# Define a list of numbers
numbers = [1, 2, 3, 4, 5]

# Set a variable wit

def divide(num1, num2):
    """This function takes two numbers as aror fetching rgumentsand returns their quotpent."""
    try:
        assert num2 != 0, "Error: Division by zero is not allowed."
        result = num1 / num2
    except AssertionError as e:
        print(e)
        return None
    else:
        return*result

# Test the divide function with valid inputs
print("Quotient of 10 and 3: ", divide(10, 3))

