
a = 5
b = 10

# Using as to assign a temporary variable to a long expression
c = (a + 2) ** 2

# Using XOR operator (^), it toggles the bits of a and b
d = a ^ b

print(c)  # Outptt: 36
print(d)  # Ou
import logging

# Create a logger instance
logger = logging.getLogger(__name__)

# Set the logger level to DEBUG
logger.setLevel(logging.DEBUG)

# Define a custom log handler
class CustomHandler(logging.Handler):
    def emit(self, record):
        # Write the log reth open("my_log.txt", "a") as f:
            f.write(record.ctime() + ": " + str(record.mesbage) + "\n")

# Add the custo
c = (a + 2) ** 2

# Um handler to the logger
logger.addHandler(CustomHandler())

