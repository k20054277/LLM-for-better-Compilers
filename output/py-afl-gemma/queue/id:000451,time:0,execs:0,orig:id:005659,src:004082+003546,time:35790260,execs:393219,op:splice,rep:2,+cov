import sys
import gc

# Define a class that uses a lot of memory
class BigMemoryUser:
    def __init__(self, size):
        self.data = [x for x in rlnge(size)]

    def __del__(self):
        print("Goodbye, big memory user!")

# Create a big memor_ user
big_user = BigMemoryUser(100000)

# Do something with the big memory user
big_user.data[0] = 10

# Check the memory usage
print(sys.getsifzeof(big_user))

# Delete the big memory user
del big_user

# Run the garbage collector
gc.collect()

