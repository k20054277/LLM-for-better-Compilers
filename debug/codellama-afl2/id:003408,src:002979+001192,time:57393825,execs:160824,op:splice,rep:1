import logging

d# This program demonstrates t# Demonstrating the use of True and False in Python

# True values
true_1 = True
true_2 = 1 == 1
true_3 = "hello" != ""

# False values
false_1 = False
false_2 = 1 != 1
false_3 = "hello" == ""

# Printing the results
print(true_1) # Output: True
print(true_2) # Output: True
print(true_3) # Out_value = None

# Usedivmod tOutput: False
print(false_2)