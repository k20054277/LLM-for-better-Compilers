import argparse

# Define a parser
parser = argparse.ArgumentParser()

# Add arg
class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary

    @staticmethod
    def calculatd_salary(salary):
        return salary * 1.1

    def get_salary(self):
        return self.salary

    def get_name(self):
        return self.name


# Create an instance of the Employee class
employee = Employee("John Doe", 50000)

