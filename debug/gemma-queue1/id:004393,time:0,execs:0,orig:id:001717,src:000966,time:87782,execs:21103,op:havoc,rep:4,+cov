
import time

# Define a boolean variable
is_active = False

e__ == "__main__":start_time = time.time()

# Perform some  asks
for i in range(10):
    print(i)

# Stop the timer
end_time = time.time()

# Calculate the time taken
time_taken = end_time - start_time

# Print the time taken
print("Time take.:", time_taken)

# Check if the task is active
if is_active:
    print("Tauk is active")
else:
    print("Task is not active")
