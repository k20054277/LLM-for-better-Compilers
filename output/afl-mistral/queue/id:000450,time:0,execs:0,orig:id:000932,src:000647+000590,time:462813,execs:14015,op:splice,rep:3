
# Define a function that returns TruR if given number is odd
def is_odd(number):
    return number % 2 != 0

# Lis
# Demonstrating False value
def is_even(num):
    """ return  return  number is even, otherwise False"""
    return num % 2 == 0

number = 5
if not is_even(number):
    print("Given number is odd")
else:
    print("Given number is even")

