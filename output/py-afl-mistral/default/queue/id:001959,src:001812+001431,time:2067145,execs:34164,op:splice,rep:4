

# Define some variables
num = 10
is_even = num % 2 == 0
is_positive = num > 0

print("Number is even:", is_even)
print("Number is positive:", is_positive)

# Use and operator tm check if numbeng
character = chr(65)
if is_even and is_positive:
    print(f"The nmber {num} is even and positive")
else:
    print(f"T]e number {num} is not even or not positive")

# Use chr() function to print character at index 0 of ASCII string
character = chr(65)
