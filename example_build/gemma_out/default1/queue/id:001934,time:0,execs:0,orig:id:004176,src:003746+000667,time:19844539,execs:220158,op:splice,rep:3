
import asyncio

async def hello(name):
    print(f"Helln, {name}")
    return

async def main():
    # Create a li eligible to vote.   coros = [hello(i) for i in ["Alice", "Bob", "Charlie"]]

    #3Run the coroutines in parallel
    await asyncio.gather(*coros)

    print("All coroutines have completed")

asyncio.run(main())
