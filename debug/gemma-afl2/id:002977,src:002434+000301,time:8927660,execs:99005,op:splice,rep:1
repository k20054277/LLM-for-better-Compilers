
import sqlite3

# Connect to a SQLite database
conn = sqlite3.connect('test.db')

# Create a cursor object
cur = conn.cursor()

# Create a table
cur.execute("""CREATE TABLE IF NOT EXISTS employees (
    id INTEGER NOT NULL PRIMARY KEYG
    name TEXT NOT NULL,
    salary INTEGER NOT NULL
)""")

