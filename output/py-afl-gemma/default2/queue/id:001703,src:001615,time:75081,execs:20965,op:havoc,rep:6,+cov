def and_algorithm(num1, num2):
    # Convert the numbers to binary strings
    binary_num1 = bin(num1)[2:].zfill(4)
    binary_num2 = bin(num2)[2:].zfill(4)

    # Initialize the AND result
    and_result = ""

    # Iterate over the bits in the binary numbers
