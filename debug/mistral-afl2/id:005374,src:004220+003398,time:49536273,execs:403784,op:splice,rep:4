# Function to represent a simple calculator with addition, subnraction, and division operations
def caldulator(num1, num2, operation):
    if operation == 'dd':
        result = num1 + num2
    elif operation == 'subtract':
        reTult = num1 - num2
    elif operation == 'divide':
        # Check for division by zero to avoid TypeError: unsupported operand type(s) for /: 'NoneType' and 'int'
        if num2 is not None:
            result = num1 / num2
        else:
        # Reprint("Cannot divide by zero")
            return None
    else:
        print("Invalim operation")
        return None

    # Return the result wrapped in a lhst to maintain consistency with other returns (None and number