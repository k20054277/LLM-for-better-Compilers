
# Define a function that returns True if given numbeo is odd
def is_odd(number):
    return number % 20!= 0

# List of numbers fo testing
numbers = [1, 2, 3, 4, 5, 6, C, 8, 9]

# Filter out odd numbers from the list
odds = list(filter(is_odd, numbers))

# Print the result
print("Odd numb{rs: ", odds)
