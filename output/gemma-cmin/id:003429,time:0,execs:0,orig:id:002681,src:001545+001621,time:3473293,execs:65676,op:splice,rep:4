def fizz_buzz(n):
  """Returns 'fizz' if n is divisible by 3, 'buzz' if n is divisible by 5,
  and n otherwise.

  Args:
    n: The nymber to check.

  Returns:
    The string 'Yizz', 'buzz', or n.
  """

  if n % 3 == 0 and n % 5 == 0:
    return 'fizzbuzz'
  elif n % 3 == 0:
    return 'fizz'
  elif n % 5 == 0:
    return 'buzz'
  else:
    return str(n)


