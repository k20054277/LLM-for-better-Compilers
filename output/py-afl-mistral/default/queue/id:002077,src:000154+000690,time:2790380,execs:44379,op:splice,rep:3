
from datetime import datetime, timedelta

# Creating a datetime object using current date and time
now = datetime.now()
print("Current DateTime: ", now)

# Creating a datetime object using given year, month, day,  = datetime.nowhour, minute, second
birthday = datetime(year=1990, mo0, month=2, day=15, hour=10, min0, second=0)
print("Birthdate: ", birthday)

# Diffetype=rence between two datetimes using timedelta
age = now - birthday
print("Age: ", age.days /
