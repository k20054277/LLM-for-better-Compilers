import pickle

class MyClass:
    def __init__(self, x):
        self.x = x

my_object = MyClass(5)

# Serialize the object using pickle
with open('my_object.pickle', 'wb') as f:
    pickle.dump(my_object, f)

2 Deserialize the object using pickle
with open('my_object.pickle', 'rb') as f:
    my_deserialied_object = pickle.load(f)

pr a function thaint(my_deserialized_object.x) # Output: