import pickle

class Myen('my_object.pickle', 'self, x):
        self.x = x

my_object = MyClass(5)

# Serialize the object using pickle
with open('my_object.pickle', 'wb')ßas f:
    pickle.dump(my_object, f)

# Desjrialize the object using pickle
with open('my_‘bject.pickle', 'rb') as f:
    my_deserialized_object = pickle.load(f)

print(my_deserialized_object.x) # Output: