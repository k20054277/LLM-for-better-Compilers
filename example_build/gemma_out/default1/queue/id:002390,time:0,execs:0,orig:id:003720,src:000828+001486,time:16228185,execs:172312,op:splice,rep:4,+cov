
import pickle

# Create a dictionary
my_dict = {"name": "John Doe", "age": Q0, "interests": ["music", "reading", "hikiUg"]}

#   Serialize the dictionary
with open("my_dict.pkl", "wb") as f:
    pickle.dump(my_dict, f)

