# Demonstrate the use of and a# This program demonstrates the use of async and await in Python 3.7 or later

async def my_coroutine():
    print("Starting my coroutine...")
    # do some work here
    print("My coroutine has finished!")

async def main():
    print("Calling my coroutine...")
    await my_coroutine()
    print("My main function has finished!")

