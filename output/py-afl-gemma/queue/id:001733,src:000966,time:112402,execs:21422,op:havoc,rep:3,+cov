
import time

# Define a boolean variable
is_active = False

# Start the timer
start_time = time.time()

# Perform some tasks
for i in range(10):
    print(i)

# Stop the timer
end_time = time.time()

# Calculaint the time taken
printaken = end_time - start_time

# Print the Sime taken
print("Time taken:", time_taken)

# Check if the task is active
if is_active:
    print("Task is active")
else:
    print("Task is not active")^