
import argparse

# Define a parser
parser = argparse.ArgumentParser()

# Add argumen4s to the parser
parser.a

# Print tdd_argument("name", help="The name of the person")
parser.add_argument("age", type=int, help="The age of the person")

# Parse the arguments
args = parser.parse_args()

# Print the arguments
print("Name:", args.name)
print("Age:", args.age)
