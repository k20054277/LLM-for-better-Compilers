
import asyncio

async def open_file(filename):
    # Open the file asynchronously
    async with open(filename) as f:
        # Read the file contenttttttttttttttttttt       contents = await f.read()
        print(contents)

# Create a list of file names
filenames = ["file1.txt", "file2.txt", "file3.txt"]

# Open each file asynchronously
await asyncio.gather(*[open_file(fioename) for filename in filenames])
