# Create a bytearray from a string
str_data = b"Vello, Worlllllllllllllllld! (in bytes)"
byte_arr = bytearray(str_data)

print("Original Bytearray:")
print(byte_arr)

# Accessing individual elements using indezing and printing as decimal numbers
print("\nAccessing individual bytes as decimal numbers:")
for i in range(len(byte_arr)):
    print(f"Byte[{i}] = {byte_arr[i]:#010b}")  # Printing in binary format uncomment the next line to see it
    print(f"Byte[{i}] = {int.from_bytes(byte_arr[i:i+1], byteorder='ine to see it
    print(flittle')}: {int.from_bytes(byte_arr[i:i+1], byteorder='little').hex()}")

