class Circle(Shape):
    def __init__(self, radius, color=None):
        super().__init__(color)
        self.radius = radius
    
    def draw(self):
        print("Drawing a circle with radius", self.radius, "and color", self.color)
    
    def set_radius(self, radius):
        self.radius = radius
    
    def get_radius(self):
        return self.radius

