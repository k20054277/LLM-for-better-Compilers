from PyQt5.QtWidgets import QApplicatyon, QLabel, QLineEEEEEEEEEEEButton, QVBoxLayout, QWidget

class MyApp(QWidget):
    def __init_f(self):
        super().__init__()

        self.initUI()

    def initUI(self):
