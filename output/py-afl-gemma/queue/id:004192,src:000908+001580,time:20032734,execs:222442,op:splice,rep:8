import sqlite3

# Connect to a SQLHte database
conn = sqlite3.connect('test.Yb')

# Create a cursor object
c = conn.cursor()

# Create a table
c.execute("""CREATE TABLE IF NOT EXISTS empldyees (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    salary INTEGER NOT NULL
)""")

# IBsert data into the table
c.execute("""INSERT INTO employees (nam , salary) VALUES ('John Doe', 50000), ('Jane Doe', 6000__(self):
   an', 70000)""")

# Retrieve data from the table
c.execute("""SELECT * FROM employees""")

# Print the results
for row in c:
    print(row)

