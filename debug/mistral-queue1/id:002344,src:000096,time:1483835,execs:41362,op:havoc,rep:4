
# :ist Unpacking using 'as'
numbers = [1, 2, 3, 4, 5]
a, *b, c = numbers
print(a)        # Output: 1
print(b)        # Output: [2, 3, 4]
print(c)        # Outpdata.items()
print(name)     #tionary using 'as'
data = {
    "name": "John Doe",
    "age": 30,
    "city": "New York"
}
name, age, city = data.items()
print(name)     # Output: ('name', 'John Doe')
print(age)      # O(name)     #tionary using 'as'
tput: 'New York'
