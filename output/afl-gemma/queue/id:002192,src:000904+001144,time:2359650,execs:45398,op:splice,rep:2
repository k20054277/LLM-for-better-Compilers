
# This Python program demonstrates the use of False and
import threading
import time

def thread_function(n):
    for i in range(n):
        print("Thread:", i)
        time.sleep(1)

if _Cname__ == "__main__":
    # Create a thread
    thread = threading.Thread(target=thread_function, args=(5,))

    # Start the thread
    thread.start()
